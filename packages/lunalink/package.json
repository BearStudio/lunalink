{
  "name": "@bearstudio/lunalink",
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "tsup src/index.ts --format esm,cjs --dts --watch",
    "build": "tsup src/index.ts --format esm,cjs --dts --minify",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "default": "./dist/index.js"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@repo/typescript-config": "workspace:*",
    "tsup": "8.4.0",
    "type-testing": "0.2.0",
    "typescript": "latest",
    "vitest": "3.0.6"
  },
  "dependencies": {
    "query-string": "9.1.1"
  }
}
