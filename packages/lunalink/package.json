{
  "name": "@bearstudio/lunalink",
  "version": "0.1.1",
  "license": "MIT",
  "description": "Lightweight TypeScript library to efficiently maintain and build URLs",
  "keywords": [
    "typescript",
    "url",
    "builder",
    "luna",
    "link",
    "parameters",
    "ts",
    "dynamic"
  ],
  "type": "module",
  "scripts": {
    "dev": "tsup src/index.ts --format esm,cjs --dts --watch",
    "build": "tsup src/index.ts --format esm,cjs --dts --minify",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "default": "./dist/index.js"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@repo/typescript-config": "workspace:*",
    "tsup": "8.4.0",
    "type-testing": "0.2.0",
    "typescript": "latest",
    "vitest": "3.0.6"
  },
  "dependencies": {
    "query-string": "9.1.1"
  },
  "author": {
    "email": "yoann@bearstudio.fr",
    "name": "Yoann Fleury",
    "url": "https://www.yoannfleury.dev"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/BearStudio/lunalink.git",
    "directory": "packages/lunalink"
  },
  "bugs": {
    "url": "https://github.com/BearStudio/lunalink/issues"
  }
}
